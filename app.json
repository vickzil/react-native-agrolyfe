{
  "expo": {
    "name": "Agrolyfe",
    "slug": "agrolyfe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/favicon.png",
    "splash": {
      "image": "./assets/img/banner.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "agrolyfe.farm.com",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/favicon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "agrolyfe.farm.com",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/img/favicon.png"
    },
    "userInterfaceStyle": "automatic",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ]
  }
}
